        <TextBlock HorizontalAlignment="Left" Margin="399,177,0,0" TextWrapping="Wrap" FontSize="30" VerticalAlignment="Top" Text="⌚"/>
        <TextBlock HorizontalAlignment="Left" Margin="329,177,0,0" TextWrapping="Wrap" FontSize="30" VerticalAlignment="Top" Text="🧍"/>
