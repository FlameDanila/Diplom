        <TextBlock HorizontalAlignment="Left" Margin="565,177,0,0" TextWrapping="Wrap" Text="📅" FontSize="30" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="703,177,0,0" TextWrapping="Wrap" FontSize="30" VerticalAlignment="Top" Text="Тип" FontFamily="Sylfaen"/>
        <TextBlock HorizontalAlignment="Left" Margin="819,177,0,0" TextWrapping="Wrap" Text="🎟" FontSize="30" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="913,177,0,0" TextWrapping="Wrap" FontSize="30" VerticalAlignment="Top" Text="Место" FontFamily="Sylfaen"/>
        <DatePicker x:Name="datePicker" HorizontalAlignment="Left" Margin="483,116,0,0" VerticalAlignment="Top" FontFamily="Sylfaen"/>
